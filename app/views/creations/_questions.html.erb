<div class="category_title"><%= render partial: "category_title", object: @active_category %></div>

<div class="panel panel-default">
    <div class="panel-heading">Add A New Question:</div>
    <div class="panel-body">
        <textarea class="question-form form-control" placeholder="Enter text ..."></textarea>
    </div>
</div>
<%= button_to "Add Question", check_question_path(category: @active_category.id), remote: true, :class => 'btn btn-primary btn-lg' %>

<% @active_category.questions.each do |q| %>
    <h3><span class="label label-default"><%= q.text.html_safe %></span></h3>
<% end %>



<script type="text/javascript">
	$.fn.wysihtml5.defaultOptions['toolbar']['image'] = false;
	console.log($.fn.wysihtml5.defaultOptions)
	$('.question-form').wysihtml5();
</script>