
	<div id="profile">
		
	</div>
	<div class="list-group">
		<% @current_rfi.categories.each do |category| %>
			 <% if @current_category != category %>
				<%= link_to category.text, update_category_path(category_id: category.id), class: "list-group-item", remote: true, method: :post %>
			<% else %>
				<%= link_to category.text, update_category_path(category_id: category.id), class: "list-group-item active", remote: true, method: :post %>
			<% end %>
			
		<% end %>
	</div>
	
