
<div class="list-group">
	<% if !@current_category.nil? %>
		
			<% @current_category.questions.each do |question| %>
				<% if @current_question != question %>
					<%= link_to question.text, question_path(id: question.id), class: "list-group-item" %>
				<% else %>
					<%= link_to question.text, question_path(id: question.id), class: "list-group-item active" %>
				<% end %>
			<% end %>
		
	<% end %>
</div>
