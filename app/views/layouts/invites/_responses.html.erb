<% content_for :questions do %>
	<% if !@current_category.nil? %>
			<% @current_category.questions.each do |question| %>
				<% if @current_question != question %>
					<%= link_to question.text, response_path(id: question.id), class: "list-group-item" %>
				<% else %>
					<%= link_to question.text, response_path(id: question.id), class: "list-group-item active" %>
				<% end %>
			<% end %>
	<% end %>
<% end %>