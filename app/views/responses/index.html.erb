
<body>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav ">
                <li class="sidebar-brand">
                    <a href="#">
                        Start Bootstrap
                    </a>
                </li>
                <li>
                    <a href="#">Quantitative</a>
                </li>
                <li>
                    <a href="#">Qualitative</a>
                </li>
                <li>
                    <a href="#">Raw Data</a>
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-11">
                        <% @questions.each do |q| %>
                            <div class="panel panel-default">
                              <div class="panel-heading"><%= q.text %></div>
                              <div class="panel-body">
                                <textarea class="some-textarea form-control" placeholder="Enter text ..."></textarea>
                                <button class="submit">Submit</button>
                              </div>
                            </div>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->

    </div>


</body>

<script type="text/javascript">
    $.fn.wysihtml5.defaultOptions['toolbar']['image']
    console.log($.fn.wysihtml5.defaultOptions)
    $('.some-textarea').wysihtml5();
</script>


